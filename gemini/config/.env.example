# Gemini API Configuration
# Copy this file to .env and fill in your credentials

# Required: Get your API key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: OAuth 2.0 (for advanced use cases)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# Model Configuration
GEMINI_DEFAULT_MODEL=gemini-2.0-flash-exp
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2048

# Rate Limiting
GEMINI_MAX_REQUESTS_PER_MINUTE=60
GEMINI_RETRY_ATTEMPTS=3
GEMINI_RETRY_DELAY_MS=1000

# Safety Settings (BLOCK_NONE, BLOCK_ONLY_HIGH, BLOCK_MEDIUM_AND_ABOVE, BLOCK_LOW_AND_ABOVE)
GEMINI_SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_DANGEROUS_CONTENT=BLOCK_MEDIUM_AND_ABOVE

# Logging
GEMINI_LOG_LEVEL=info
GEMINI_LOG_REQUESTS=false
GEMINI_LOG_RESPONSES=false

# Cache Settings
GEMINI_ENABLE_CACHE=true
GEMINI_CACHE_TTL_SECONDS=3600

# Project Settings
PROJECT_NAME=DevSkyy
ENVIRONMENT=development
