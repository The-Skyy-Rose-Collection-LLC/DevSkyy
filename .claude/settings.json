{
  "mcpServers": {
    "devskyy-main": {
      "command": "python",
      "args": ["devskyy_mcp.py"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "TRIPO_API_KEY": "${TRIPO_API_KEY}",
        "FASHN_API_KEY": "${FASHN_API_KEY}",
        "WORDPRESS_URL": "${WORDPRESS_URL}",
        "WOOCOMMERCE_KEY": "${WOOCOMMERCE_KEY}",
        "WOOCOMMERCE_SECRET": "${WOOCOMMERCE_SECRET}",
        "ENVIRONMENT": "production"
      }
    },
    "devskyy-openai": {
      "command": "python",
      "args": ["mcp_servers/openai_server.py"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "ENVIRONMENT": "production"
      }
    },
    "devskyy-rag": {
      "command": "python",
      "args": ["mcp_servers/rag_server.py"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "VECTOR_DB_PATH": "./data/vectordb",
        "RAG_COLLECTION": "devskyy_docs",
        "EMBEDDING_PROVIDER": "sentence_transformers"
      }
    },
    "devskyy-agents": {
      "command": "python",
      "args": ["mcp_servers/agent_bridge_server.py"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
      }
    },
    "devskyy-woocommerce": {
      "command": "python",
      "args": ["mcp_servers/woocommerce_mcp.py"],
      "env": {
        "WORDPRESS_URL": "${WORDPRESS_URL}",
        "WOOCOMMERCE_KEY": "${WOOCOMMERCE_KEY}",
        "WOOCOMMERCE_SECRET": "${WOOCOMMERCE_SECRET}"
      }
    }
  },
  "enabledPlugins": {
    "agent-sdk-dev@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "devops-autopilot@corey-local": true,
    "fullstack-deployer@corey-local": true,
    "immersive-architect@corey-local": true,
    "security-guidance@corey-local": true
  }
}
