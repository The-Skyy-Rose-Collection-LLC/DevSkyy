{
  "rewrites": [
    {
      "source": "/experiences/black-rose",
      "destination": "/experiences/black-rose.html"
    },
    {
      "source": "/experiences/love-hurts",
      "destination": "/experiences/love-hurts.html"
    },
    {
      "source": "/experiences/signature",
      "destination": "/experiences/signature.html"
    },
    {
      "source": "/collections/:path*",
      "destination": "/collections/:path*.html"
    },
    {
      "source": "/preorder",
      "destination": "/pre-order.html"
    },
    {
      "source": "/pre-order",
      "destination": "/pre-order.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}
