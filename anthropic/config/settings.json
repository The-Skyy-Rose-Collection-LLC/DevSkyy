{
  "defaultModel": "claude-sonnet-4-6",
  "fallbackModel": "claude-haiku-4-5",
  "baseURL": "https://api.anthropic.com",
  "generationConfig": {
    "max_tokens": 4096,
    "temperature": 0.7
  },
  "rateLimit": {
    "requestsPerMinute": 50,
    "retryAttempts": 3,
    "retryDelayMs": 1000,
    "exponentialBackoff": true
  },
  "cache": {
    "enabled": true,
    "ttlSeconds": 3600,
    "maxSize": 1000
  },
  "logging": {
    "level": "info",
    "logRequests": false,
    "logResponses": false,
    "logTokenUsage": true
  },
  "streaming": {
    "enabled": true,
    "chunkSize": 512
  }
}
